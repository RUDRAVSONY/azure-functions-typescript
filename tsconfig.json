{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true
  },
  "awesomeTypescriptLoaderOptions": {
    "usePrecompiledFiles": true,
    "useWebpackText": true
  }
}
